net : "det3-train.prototxt"
test_iter: 50
test_interval: 500
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 20000
gamma: 0.8
display: 100   #500
max_iter: 600000
snapshot: 5000
snapshot_prefix: "./models-48/"
solver_mode: GPU
