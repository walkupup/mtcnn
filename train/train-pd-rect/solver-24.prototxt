net : "det2-train.prototxt"
test_iter: 50
test_interval: 500
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.004
# The learning rate policy
lr_policy: "step"
stepsize: 10000
gamma: 0.8
display: 100   #500
max_iter: 500000
snapshot: 5000
snapshot_prefix: "./models-24/"
solver_mode: GPU
